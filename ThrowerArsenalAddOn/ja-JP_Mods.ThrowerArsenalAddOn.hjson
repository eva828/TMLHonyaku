Configs: {
	Config: {
		DisplayName: 設定

		hhgRework: {
			Label: 聖なる手榴弾のダメージクラスを変更
			Tooltip:
				'''
				この武器のダメージクラスを投擲武器に変更します
				また、ダメージなどの他のステータスも武器として使用できるように調整されます
				さらに、攻撃の挙動も調整されます
				特に注目すべきは、ブロックを破壊しなくなったことです
				'''
		}

		daggerConv: {
			Label: サンギュインダガーのダメージクラスを変更
			Tooltip:
				'''
				この武器のダメージクラスを投擲武器に変更します
				元々のダメージタイプにより、この武器は変換時に汎用（つまりクラス固有でない）の修飾子のみが付きます
				'''
		}

		tridentConv: {
			Label: 呪われたトライデントのダメージクラスを変更
			Tooltip:
				'''
				この武器のダメージクラスを投擲武器に変更します
				元々のダメージタイプにより、この武器は変換時に汎用（つまりクラス固有でない）の修飾子のみが付きます
				'''
		}

		flaskConv: {
			Label: 燃焼のフラスコのダメージクラスを変更
			Tooltip:
				'''
				この武器のダメージクラスを投擲武器に変更します
				更にマナの消費も少なくなるが、それに応じてダメージも減少します
				元々のダメージタイプにより、この武器は変換時に汎用（つまりクラス固有でない）の修飾子のみが付きます
				'''
		}

		slimenobiConv: {
			Label: スライムノビのダメージクラスを変更
			Tooltip: この武器のダメージクラスを投擲武器に変更します
		}
	}
}

