Configs: {
	Modconfig: {
		DisplayName: 設定

		Headers: {
			Hurt: "[i:29] ダメージオーバーレイ"
			Low: "[i:29] 低HPオーバーレイ"
			Bar: "[i:705] プレイヤーHPバー"
			Potion: "[i:28] ポーションポップアップ"
			Debuff:
				'''
				[i:2701] デバフポップアップ
				(注: Fargo's Mutant Modには類似機能があります
				動作しない場合は、そちらの設定を確認してください)
				'''
			Gray: "[i:1099] 灰色の視界"
		}

		DisableMLShader: {
			Label: ムーンロードのシェーダーを無効化
			Tooltip:
				'''
				ムーンロードのシェーダーが原因で、以下の2つのオーバーレイが表示されなくなる可能性があります
				オンにすると無効になります
				'''
		}

		EnableHurtOverlay: {
			Label: 被ダメージ時のオーバーレイを有効化
			Tooltip: 
				'''
				ダメージを受けた際、画面の端が赤く染まり、
				素早くフェードアウトします
				'''
		}

		HurtOverlayType: {
			Label: 使用オーバーレイ
			Tooltip:
				'''
				オーバーレイの種類を選択
				HPOverlay - 単色で画面の端を覆う (レトロ/トリッピー照明モード推奨)
				New - グラデーションで画面の端を覆う (カラー/ホワイト照明モードのみ)
				Flat - 単色で画面全体を覆う
				'''
		}

		HurtColor: {
			Label: "[i:1073] オーバーレイの色"
			Tooltip: 注: レトロ／トリッピーライティングを使用していない場合、アルファ値は影響しません
		}

		HurtUseDarkColors: {
			Label: "[i:1085] より暗い色を使用"
			Tooltip:
				'''
				※ 実験的機能
				レトロ／トリッピーライティングでは非対応
				最も正確に表示するにはアルファを255に設定してください（シェーダーには影響しません）
				白 = 透明
				'''
		}

		HurtAlpha: {
			Label: オーバーレイの強度
			Tooltip:
				'''
				オーバーレイの色の強度を設定できます
				数値が高いほど強く表示されます
				'''
		}

		HurtSpeed: {
			Label: オーバーレイのフェード速度
			Tooltip:
				'''
				オーバーレイの消滅速度を設定できます
				数値が高いほど速く消滅します
				'''
		}

		HaveIntensity: {
			Label: 可変強度を使用
			Tooltip:
				'''
				ダメージオーバーレイの強度が、ダメージの頻度によって変化します
				高い強度設定と併用推奨
				カラー/ホワイト照明モードのみ
				'''
		}

		EnableLowHpOverlay: {
			Label: 低HP時のオーバーレイを有効化
			Tooltip: 画面の端が赤く点滅します
		}

