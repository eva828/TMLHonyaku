Configs: {
	CoJConfigs: {
		UseAltOverwatcherMusic: {
			Tooltip: ""
			Label: 監視者に別テーマのBGMを使用
		}

		EnableMasterAIInExpert: {
			Tooltip:
				'''
				これらのAI用ステータスはエキスパート向けに設計されていない点に注意
				想定外に大きくバランスが崩れる可能性がある
				異常なダメージを確認した場合は報告してほしい
				将来的に徹底的な修正を行う予定
				'''
			Label: エキスパートでマスターAIを有効化
		}

		OverwatcherMasterAIToggle: {
			Tooltip: マスターモードで監視者が専用AIを使用するかを切り替える
			Label: 監視者マスターAI切り替え
		}

		UseAltWhaleMusic: {
			Tooltip: ""
			Label: 果てなき墜落の鯨に別テーマを使用
		}

		WorldsEndEverlastingFallingWhaleMasterAIToggle: {
			Tooltip: 果てなき墜落の鯨がマスターモードで専用AIを使用するか切り替える
			Label: 果てなき墜落の鯨AI切替
		}

		TheSonMasterAIToggle: {
			Tooltip: 現在この設定は機能していない
			Label: 御子AI切替
		}

		SlimeGodEagleSoundVolumn: {
			Tooltip: ""
			Label: スライムの神：効果音音量
		}

		TheLifebringerMasterAIToggle: {
			Tooltip: 命をもたらすものがマスターモードで専用AIを使用するか切り替える
			Label: 命をもたらすものAI切替
		}

		AirblastDamageMaximumMultiplier: {
			Tooltip:
				'''
				この設定値は反射された飛び道具のダメージ倍率の上限を制限する
				反射ダメージは進行に応じて上昇する
				通常プレイでは7のまま推奨、この設定はテスト用
				0 = x1
				1 = x6.66 - クトゥルフの目、イーターオブワールド/クトゥルフの脳、スケルトロン撃破後
				2 = x9 - ウォールオブフレッシュ撃破後
				3 = x10 - ツインズ、デストロイヤー、スケルトロンプライム撃破後
				4 = x12 - ゴーレム撃破後
				5 = x50 - マザーブレイン撃破後
				6 = x90 - ウォールオブシャドウ撃破後
				7 = x120 - 監視者、命をもたらすもの、具現者撃破後
				'''
			Label: 反射弾ダメージ倍率上限
		}

		AbyssAtTheSameSideWithJungle: {
			Tooltip: ""
			Label: 深淵をジャングルと同じ側に生成
		}

		SlimeGodMasterAIToggle: {
			Tooltip: スライムの神がマスターモードで専用AIを使用するか切り替える
			Label: スライムの神AI切替
		}

		TheMotherbrainMasterAIToggle: {
			Tooltip: マザーブレインがマスターモードで専用AIを使用するか切り替える
			Label: マザーブレインAI切替
		}

		AirblastEnabled: {
			Tooltip: 火炎放射器を持って右クリックしたときに圧縮噴射するか切り替える
			Label: 圧縮噴射有効化
		}

		SpearEnabled: {
			Tooltip: 槍を持って右クリックしたときにダッシュするか切り替える
			Label: 槍のダッシュ有効化
		}

		TheTwinsMasterAIToggle: {
			Tooltip: ツインズがマスターモードで専用AIを使用するか切り替える
			Label: ツインズ
		}

		DisplayName: Homeward Journeyサーバー設定
	}
}

